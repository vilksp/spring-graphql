schema {
    query: Query
}
type Actor{
    id:ID!,
    firstName:String,
    lastName:String,
    dateOfBirth:String,
    address:String,
    filmId:Int
}
type Film{
    filmId:ID!,
    film_name:String,
    releaseYear:String

}
type Query{
    getAllActors:[Actor],
    getActorById(id:Float):Actor,
    getAllFilms:[Film]

}